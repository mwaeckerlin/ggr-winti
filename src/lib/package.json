{
  "name": "@ggr-winti/lib",
  "version": "0.0.1",
  "description": "LaTeX Input Encoding Library for Vorstoss Generator",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "start": "rollup -c",
    "start:dev": "rollup -c -w",
    "build": "rollup -c",
    "clean": "rm -rf dist",
    "clean:all": "npm run clean && rm -rf node_modules",
    "format": "prettier --write \"src/**/*.ts\"",
    "test": "echo 'No tests yet' && exit 0"
  },
  "keywords": [
    "latex",
    "encoding",
    "validation",
    "vorstoss"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@rollup/plugin-commonjs": "28.0.6",
    "@rollup/plugin-node-resolve": "16.0.1",
    "@rollup/plugin-typescript": "12.1.3",
    "prettier": "3.5.3",
    "rollup": "4.44.0"
  },
  "dependencies": {
    "class-validator": "0.14.2",
    "base64-js": "1.5.1"
  }
}